{
  "name": "arc",
  "version": "0.22.5",
  "description": "sensible framework for team work",
  "main": "index.html",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "start": "firebase emulators:exec --only firestore \"npm run dev:app\"",
    "dev:app": "webpack-dev-server --config build/webpack.dev.js",
    "dev:test": "concurrently -n \"app,test\" -c \"bgYellow.black,bgWhite.black\" \"npm:dev:app\" \"npx cypress open\"",
    "lint": "eslint --ext .js,.vue src",
    "build": "npm run lint && rimraf dist/* && webpack --config build/webpack.prod.js",
    "deploy": "npm run build && firebase deploy",
    "commit": "npm run lint && git add -A && git commit",
    "push": "git push && git push --tags",
    "bump:patch": "npm run lint && npm run commit && npm version patch && npm run push",
    "bump:minor": "npm run lint && npm run commit && npm version minor && npm run push",
    "release:patch": "npm run bump:patch && npm run deploy",
    "release:minor": "npm run bump:minor && npm run deploy"
  },
  "author": "j@vmelo.net",
  "license": "ISC",
  "dependencies": {
    "@ttskch/select2-bootstrap4-theme": "^1.3.2",
    "bootstrap": "^4.4.1",
    "bootstrap-daterangepicker": "^3.0.5",
    "firebase": "^6.6.2",
    "firebaseui": "^4.3.0",
    "jquery": "^3.4.1",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0",
    "popper.js": "^1.16.0",
    "select2": "^4.0.12",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.1",
    "vue-loading-overlay": "^3.2.0",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.2",
    "cypress": "^3.8.0",
    "dotenv-webpack": "^1.7.0",
    "es-check": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "eslint-plugin-vue-libs": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
