{
  "name": "arc",
  "version": "0.20.1",
  "description": "sensible framework for team work",
  "main": "index.html",
  "browserslist": [
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.dev.js",
    "lint": "eslint --ext .js,.vue src",
    "build": "npm run lint && rimraf dist/* && webpack --config build/webpack.prod.js",
    "deploy": "npm run build && firebase deploy",
    "commit": "npm run lint && git add -A && git commit",
    "push": "git push && git push --tags",
    "bump:patch": "npm run lint && npm run commit && npm version patch && npm run push",
    "bump:minor": "npm run lint && npm run commit && npm version minor && npm run push",
    "release:patch": "npm run bump:patch && npm run deploy",
    "release:minor": "npm run bump:minor && npm run deploy"
  },
  "author": "j@vmelo.net",
  "license": "ISC",
