{
  "name": "arc",
  "version": "0.22.49",
  "description": "ARC is an experimental web app that aims to help individuals, organizations, communities and families to collaboratively organize their work pipeline using an impact driven mindset.",
  "main": "index.html",
  "scripts": {
    "start": "npm run dev:build",
    "dev:build": "rimraf build/* && webpack --env dev --config webpack.config.js",
